<core:FragmentDefinition xmlns="sap.m" xmlns:custom="zhvz0020.controller" xmlns:core="sap.ui.core"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<P13nDialog ok="handleOK" cancel="handleCancel" showReset="true" reset="handleReset" initialVisiblePanelType="custom1">
		<panels>
			<P13nColumnsPanel visible="true" addColumnsItem="onAddColumnsItem" changeColumnsItems="onChangeColumnsItem" type="columns"
				items="{ path: '/Customer', sorter: { path: 'key' } }">
				<items>
					<P13nItem columnKey="{text}" key="{text}" values="{text}" text="{text}"/>
				</items>
			</P13nColumnsPanel>
		</panels>
	</P13nDialog>
</core:FragmentDefinition>